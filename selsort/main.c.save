#include <stdio.h>
#include <stdlib.h>
#define N 10

int main()
{
    int i,j,a[N],b=0;
    printf("Please input %d numbers:",N);
    for(i=0;i<N;i++)
        scanf("%d",a[i]);
    for (i=0;i<N-1;i++){
        for(j=i+1;j<N;j++){
            if(a[i]<a[j]){
                b=a[i];
                a[i]=a[j];
                a[j]=b;
            }
        }
    }
    for(i=0;)
    printf("Hello world!\n");
    return 0;
}
